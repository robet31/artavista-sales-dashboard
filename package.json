{
  "name": "artavista-sales-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k \"npm run backend\" \"npm run frontend\"",
    "frontend": "npx next dev --webpack -p 3000",
    "setup": "npm install && cd backend-fastapi && pip install -r requirements.txt",
    "setup:win": "setup.bat",
    "setup:unix": "bash setup.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "cd backend-fastapi && python -m uvicorn app.main:app --host 127.0.0.1 --port 8000",
    "start:all": "npm run dev"
  },
